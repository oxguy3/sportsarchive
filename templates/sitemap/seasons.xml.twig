<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url>
    <loc>{{ url('season_sportpicker') }}</loc>
    <changefreq>weekly</changefreq>
  </url>
  <url>
    <loc>{{ url('season_list') }}</loc>
    <changefreq>weekly</changefreq>
  </url>
  {% for sc in sportCounts %}
    {% set sport = sc.sport is not null ? sc.sport : "_none" %}
    <url>
      <loc>{{ url('season_list_sport', {sport: sport}) }}</loc>
      <changefreq>weekly</changefreq>
    </url>
  {% endfor %}
  {% for season in seasonsAll %}
    <url>
      <loc>{{ url('season_show', {season: season.year}) }}</loc>
      <changefreq>weekly</changefreq>
    </url>
  {% endfor %}
  {% for season in seasonsSport %}
    {% set sport = season.sport is not null ? season.sport : "_none" %}
    <url>
      <loc>{{ url('season_show_sport', {sport: sport, season: season.year}) }}</loc>
      <changefreq>weekly</changefreq>
    </url>
  {% endfor %}
</urlset>
