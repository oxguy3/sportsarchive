{% extends "base.html.twig" %}

{% block title %}{{ 'seasonList.title'|trans }}{% endblock %}

{% block body %}
  <h1>{{ 'seasonList.title'|trans }}</h1>
  <ul>
    {% for year in years %}
      <li>
        <a href="/seasons/{{ year.year }}">
          {{- year.year -}}
        </a>
      </li>
    {% endfor %}
  </ul>
{% endblock %}
