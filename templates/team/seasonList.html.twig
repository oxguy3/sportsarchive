{% extends "base.html.twig" %}

{% block title %}Headshots by season{% endblock %}

{% block body %}
  <h1>Headshots by season</h1>
  <ul>
    {% for year in years %}
      <li>
        <a href="/seasons/{{ year.year }}">
          {{- year.year -}}
        </a>
      </li>
    {% endfor %}
  </ul>
{% endblock %}
